{
  "name": "@plantquest/geofence-display",
  "version": "0.0.1",
  "description": "Geofence Display",
  "type": "module",
  "main": "./dist/geofence-display.umd.cjs",
  "module": "./dist/geofence-display.js",
  "types": "./dist/geofence-display.d.ts",
  "exports": {
    ".": {
      "import": "./dist/geofence-display.js",
      "require": "./dist/geofence-display.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite dev",
    "watch": "tsc -w & vite dev",
    "build": "tsc && vite build",
    "test-web": "npm run build-web && browserify -i util -o test/verify/web.js -e test/verify/entry.js -im && see test/verify/web.html",
    "build-web": "browserify -i util -o test/verify/leaflet.js -e test/verify/leaflet.web.js -s Leaflet -i",
    "redev": "npm run build && cd dev/ && npm run dev"
  },
  "author": "RÃ­ona Stokes (https://github.com/rionastokes)",
  "license": "MIT",
  "devDependencies": {
    "@types/geojson": "^7946.0.12",
    "@types/jest": "^29.5.7",
    "@types/leaflet": "^1.9.7",
    "@types/node": "^20.8.10",
    "browserify": "^17.0.0",
    "jest": "^29.7.0",
    "leaflet": "^1.9.4",
    "ts-jest": "^29.1.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.3",
    "vitest": "^0.34.6"
  }
}
